{
  "name": "jonathan-biro",
  "version": "2.0.0",
  "description": "Jonathan Biro Portfolio",
  "main": "index.js",
  "scripts": {
    "build": "cleandir ./.cache && gatsby build",
    "develop": "cleandir ./.cache && gatsby develop",
    "start": "npm run develop",
    "lint": "eslint ./src",
    "setup": "node ./bin/setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonbiro/personal-site.git"
  },
  "keywords": [
    "gatsby",
    "biro",
    "web",
    "developer",
    "portfolio"
  ],
  "author": "Jonathan Biro<j@biro.dev>",
  "license": "MIT",
  "homepage": "https://github.com/jonbiro",
  "dependencies": {
    "clean-dir": "^1.0.3",
    "contentful": "^7.6.0",
    "dotenv": "^8.0.0",
    "envfile": "^3.0.0",
    "gatsby": "^2.8.8",
    "gatsby-plugin-google-analytics": "^2.0.21",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.1",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-source-contentful": "^2.0.69",
    "gatsby-source-medium": "^2.0.8",
    "gatsby-transformer-remark": "^2.3.12",
    "inquirer": "^6.3.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-fontawesome": "^1.6.1",
    "react-headroom": "^2.2.8",
    "react-helmet": "^5.2.1",
    "react-markdown": "^4.0.8",
    "react-reveal": "^1.2.2",
    "react-scroll-section": "^1.3.0",
    "react-text-loop": "^2.0.2",
    "react-tippy": "^1.2.3",
    "rebass": "^3.1.1",
    "styled-components": "^4.3.1"
  },
  "devDependencies": {
    "chalk": "^2.4.2",
    "contentful-import": "^7.4.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.4.0",
    "lint-staged": "^8.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
